<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.bucao_springboot.mapper.ManagerInfoMapper">

    <resultMap id="BaseResultMap" type="com.example.bucao_springboot.entity.ManagerInfo">
            <id property="id" column="id" jdbcType="VARCHAR"/>
            <result property="psd" column="psd" jdbcType="VARCHAR"/>
            <result property="portrait" column="portrait" jdbcType="VARCHAR"/>
            <result property="telephone" column="telephone" jdbcType="VARCHAR"/>
            <result property="address" column="address" jdbcType="VARCHAR"/>
            <result property="sex" column="sex" jdbcType="CHAR"/>
    </resultMap>

    <!--查询所有用户信息-->
    <select id="queryManagerInfo" resultType="com.example.bucao_springboot.entity.ManagerInfo">
        select *
        from Manager_Info
    </select>


    <!--批量插入信息-->
    <insert id="addManagerInfo" parameterType="java.util.List">
        insert into Manager_Info
        values
        <foreach collection="list" item="item" index= "index" separator =",">
            (
            #{item.id},
            #{item.psd},
            #{item.portrait},
            #{item.telephone},
            #{item.address},
            #{item.sex}
            )
        </foreach>
    </insert>


    <sql id="Base_Column_List">
        id,psd,portrait,
        telephone,address,sex
    </sql>
</mapper>
